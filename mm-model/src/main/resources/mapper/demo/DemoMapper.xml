<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wdsite.demo.mapper.DemoMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.wdsite.demo.entity.Demo">
    <result column="id" jdbcType="bigint(20) unsigned" property="id" />
        <result column="name" jdbcType="varchar(50)" property="name" />
        <result column="REMOVED" jdbcType="bit(1)" property="removed" />
        <result column="create_time" jdbcType="timestamp" property="createTime" />
        <result column="update_time" jdbcType="timestamp" property="updateTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id,
        name, REMOVED, create_time, update_time
    </sql>

</mapper>
